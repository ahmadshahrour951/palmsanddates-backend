{
  "name": "palmsanddates-backend",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/ahmadshahrour951/palmsanddates-backend.git",
  "author": "shahrour <ahmadshahrour951@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "clean": "rm -rf ./dist && mkdir dist",
    "build": "yarn run clean && babel ./src --out-dir dist",
    "start": "yarn run build && nodemon --exec babel-node ./src/server.js",
    "serve": "node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development yarn start",
    "predev": "cross-env NODE_ENV=development yarn run db:reset:dev",
    "db:reset:dev": "yarn sequelize-cli db:drop && yarn sequelize-cli db:create && yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all --seeders-path ./src/db/seeders/dev",
    "db:create:dev": "cross-env NODE_ENV=development yarn sequelize-cli db:create",
    "test": "cross-env NODE_ENV=test mocha ./src/tests/* --require @babel/register",
    "pretest": "cross-env NODE_ENV=test yarn run db:reset:test",
    "db:reset:test": "yarn sequelize-cli db:drop && yarn sequelize-cli db:create && yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all --seeders-path ./src/db/seeders/test",
    "db:create:test": "cross-env NODE_ENV=test yarn sequelize-cli db:create",
    "db:migrate:prod": "yarn sequelize-cli db:migrate"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.7",
    "prettier": "2.3.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
