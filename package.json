{
  "name": "palmsanddates-backend",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/ahmadshahrour951/palmsanddates-backend.git",
  "author": "shahrour <ahmadshahrour951@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development yarn nodemon server.js",
    "predev": "cross-env NODE_ENV=development yarn run db:reset:dev",
    "db:reset:dev": "yarn sequelize-cli db:drop && yarn sequelize-cli db:create && yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all --seeders-path ./app/db/seeders/dev",
    "db:create:dev": "cross-env NODE_ENV=development yarn sequelize-cli db:create",
    "test": "cross-env NODE_ENV=test mocha ./app/tests/*",
    "pretest": "cross-env NODE_ENV=test yarn run db:reset:test",
    "db:reset:test": "yarn sequelize-cli db:drop && yarn sequelize-cli db:create && yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all --seeders-path ./app/db/seeders/test",
    "db:create:test": "cross-env NODE_ENV=test yarn sequelize-cli db:create"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
